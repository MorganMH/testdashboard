{
  "build": {
    "devPath": "../FrontendDevNew",
    "distDir": "../FrontendDevNew",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "tauri-dash",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "windows": [
      {
        "title": "pulse dash",
        "width": 700,
        "height": 750
      }
    ],
    "security": {
      "csp": "default-src 'self'; img-src 'self' asset: https://asset.localhost"
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "bppulsedashboard.tauri.dev",
      "icon": [
        
        "icons/icon.ico"
      ]
    }
  }
}
